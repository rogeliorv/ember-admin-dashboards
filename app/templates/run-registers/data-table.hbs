<div class="table-responsive">
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>Time</th>
        <th>Distance</th>
        <th>Average Speed</th>
        <th>Date</th>
        <th>Location</th>
        <th colspan="2"> Actions </th>

      </tr>
    </thead>
    <tbody>
      {{#each model as |runRegister|}}

        <tr>
          <td> {{runRegister.time}} minutes </td>
          <td> {{runRegister.distance}} km</td>
          <td> {{runRegister.speedFixed}} km/h</td>
          <td> {{runRegister.date}} </td>
          <td> {{runRegister.location}}</td>
          <td> {{# link-to "runRegisters.edit" runRegister }}<i class="fa fa-pencil"></i>{{/link-to}}</td>
          <td> <a href="#" {{action "remove" runRegister}}><i class="fa fa-trash"></i></a> </td>


        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
